{
  "name": "restapp-boilerplate",
  "version": "2.0.1",
  "description": "A RESTApp boilerplate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strt/restapp-boilerplate.git"
  },
  "scripts": {
    "setup": "node util/setup.js",
    "zip": "npm run transpile && node util/zip.js",
    "deploy": "npm run zip && node util/deploy.js",
    "force-deploy": "npm run zip && node util/deploy.js force",
    "create-addon": "node util/create-addon.js",
    "sign": "npm run zip && node util/sign.js",
    "prod-deploy": "node util/prod-deploy.js",
    "transpile": "npx babel ./src -d ./compiled --copy-files --delete-dir-on-start",
    "precommit": "lint-staged"
  },
  "license": "MIT",
  "author": "Strateg Marknadsf√∂ring AB",
  "contributors": [
    "Alexander Karlsson <alexander.karlsson@strateg.se>",
    "Alexander Nanberg <alexander.nanberg@strateg.se>",
    "Jesper Johansson <jesper.johansson@strateg.se>"
  ],
  "bugs": {
    "url": "https://github.com/strt/restapp-boilerplate/issues"
  },
  "homepage": "https://github.com/strt/restapp-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-strateg": "^1.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "inquirer": "^5.2.0",
    "jsonfile": "^4.0.0",
    "lint-staged": "^7.0.5",
    "prettier": "^1.14.2",
    "request": "^2.87.0",
    "zip-dir": "^1.0.2"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --config '.eslintrc' --fix",
      "git add"
    ]
  }
}
